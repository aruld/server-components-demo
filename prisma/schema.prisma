// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model notes {
  id         Int     @id @default(autoincrement())
  created_at String
  updated_at String
  title      String?
  body       String?
}
